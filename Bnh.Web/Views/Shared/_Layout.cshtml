<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - Brand New Homes</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
        <link href="~/css/styles.css" rel="stylesheet" type="text/css" />
        @RenderSection("Styles", false)
        @RenderSection("_MsCmsStyles", false)
        <meta name="viewport" content="width=device-width" />
        <script src="~/js/libs/require/require.min.js" data-main="@Url.Content("~/js/main")"></script>
    </head>
    <body>

        <div class="header navbar navbar-static-top">
            <div class="navbar-inner">
                <div class="container">
                    <div class="row">
                        <div class="span4">
                            <div class="logo"></div>
                        </div>
                        <div class="span8">
                            <div class="pull-right right">
                                <div>@Html.Partial("_LoginPartial")</div>
                                <nav>
                                    <ul class="nav">
                                        @Html.ActionMenuItem("Home", "Index", "Home")
                                        @Html.ActionMenuItem("Communities", "Index", "Community")
                                        @Html.ActionMenuItem("Search", "About", "Home")
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>


@*                    
                    <div>
                        
                    </div>
                </div>
                <div class="container">
                    
                </div>*@
            </div>
        </div>

        <div id="body" class="container">
            @RenderBody()

            <div class="navbar navbar-static-bottom">
                <div class="navbar-inner">
                    <div class="container">
                        <footer>
                            <span>© BrandNewHomes.cs 2012</span>
                        </footer>
                    </div>
                </div>
            </div>
        </div>

        @RenderSection("Scripts", false)
        @RenderSection("_MsCmsScripts", false)
    </body>
</html>